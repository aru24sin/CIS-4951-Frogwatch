rules_version = '2';
service firebase.storage {
  match /b/{bucket}/o {
    // Your app uploads to uploaded_audios/...
    match /uploaded_audios/{allPaths=**} {
      allow write: if request.auth != null;  // must be signed in
      allow read:  if true;                  // or require auth if you want it private
    }
  }
}
